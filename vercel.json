{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/App.tsx",
            "methods": [
                "GET",
                "HEAD",
                "PUT",
                "PATCH",
                "POST",
                "DELETE",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, Accept, Authorization",
                "Access-Control-Allow-Credentials": "true"
            }
        },
        {
            "src": "/validar/(.*)",
            "dest": "src/App.tsx",
            "methods": [
                "GET",
                "HEAD",
                "PUT",
                "PATCH",
                "POST",
                "DELETE",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, Accept, Authorization",
                "Access-Control-Allow-Credentials": "true"
            }
        }
    ]
}